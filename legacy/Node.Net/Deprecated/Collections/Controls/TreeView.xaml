<TreeView x:Class="Node.Net.Collections.Controls.TreeView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Node.Net.Collections.Controls"
             xmlns:nnc="clr-namespace:Node.Net.Collections"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <TreeView.ItemTemplate>
        <HierarchicalDataTemplate DataType="{x:Type nnc:Dictionary}" ItemsSource="{Binding ItemsSource}">
            <StackPanel Orientation="Horizontal">
                <StackPanel.ContextMenu>
                    <ContextMenu>
                        <MenuItem Header="Test"></MenuItem>
                    </ContextMenu>
                </StackPanel.ContextMenu>
                <Label Content="{Binding Key}" Margin="0" Padding="0" Foreground="LightGray" FontSize="16"/>
            </StackPanel>
        </HierarchicalDataTemplate>
    </TreeView.ItemTemplate>
</TreeView>
